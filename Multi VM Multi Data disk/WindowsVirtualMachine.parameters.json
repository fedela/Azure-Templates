{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "virtualMachineAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "keyvault-resourceID"
        },
        "secretName": "labadmin"
      }
    },
    "virtualMachineNamePrefix": {
      "value": "MyVM0"
    },
    "virtualMachineCount": {
      "value": 3
    },
    "virtualMachineSize": {
      "value": "Standard_DS3_v2"
    },
    "availabilitySetName": {
      "value": "MyAvailabilitySet"
    },
    "dataDiskCount": {
      "value": 2
    },
    "dataDisksSizeGB": {
      "value": 10
    },
    "dataDisksType": {
      "value": "Premium_LRS"
    },
    "vNetName": {
      "value": "MyVNET"
    },
    "SubnetName": {
      "value": "Subnet"
    }
  }
}